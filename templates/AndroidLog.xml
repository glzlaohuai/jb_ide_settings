<templateSet group="AndroidLog">
  <template name="logui" value="LogUtils.i(TAG, &quot;$content$&quot;);" description="LogUtils.i(TAG, String)" toReformat="true" toShortenFQNames="true">
    <variable name="content" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>